{
  "name": "nitrosoft",
  "version": "1.0.0",
  "description": "IT Solutions and Services Company",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch sass/main.scss css/main.css",
    "compile:sass": "sass sass/main.scss css/main.comp.css",
    "prefix:css": "postcss css/main.comp.css --use autoprefixer --output css/main.prefix.css",
    "compress:css": "sass --style=compressed css/main.prefix.css css/main.min.css",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "browserslist": [
    "last 20 versions"
  ],
  "author": "Ahmed Rezk",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.7"
  }
}
